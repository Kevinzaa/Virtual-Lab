{
  "expo": {
    "name": "VirtualLab",
    "slug": "virtuallab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_white.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1f263c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jsm.virtuallab"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jsm.virtuallab"
    },
    "web": {
      "bundler": "metro",
      "build": {
        "output": "static"
      },
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "your-eas-project-id-here"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "platforms": ["ios", "android", "web"]
  }
}