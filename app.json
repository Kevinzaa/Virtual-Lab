{
  "expo": {
    "name": "virtuallab",
    "scheme": "virtuallab",
    "slug": "virtuallab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_white.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1f263c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jsm.virtuallab"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_white.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jsm.virtuallab"
    },
    "web": {
      "bundler": "metro",
      "build": {
        "output": "static"
      },
      "favicon": "./assets/icon_white.png"
    },
    "extra": {
      "eas": {
        "projectId": "16abf549-8e63-41c7-bca2-40ca7f5b98cd"
      },
      "router": {
        "origin": false
      }
    },
    "plugins": [
      "expo-router"
    ],
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "owner": "kevinza"
  }
}
